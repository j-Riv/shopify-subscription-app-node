{
  "name": "shopify-subscription-app-node",
  "description": "Embedded Shopify App made with Node and TypeScript",
  "author": "j-Riv <jose.alfredo.rivera@gmail.com>",
  "private": true,
  "scripts": {
    "ts": "tsc -w",
    "build": "tsc && npm run build:client",
    "build:client": "vite build --outDir dist/client",
    "debug": "node --inspect-brk server/index.js",
    "newDev": "cross-env NODE_ENV=development nodemon",
    "dev": "cross-env NODE_ENV=development nodemon dist/server/index.js --watch ./dist/server",
    "prepare": "cd .. && husky install",
    "serve": "cross-env NODE_ENV=production node dist/server/index.js",
    "start": "npm run serve",
    "test": "vitest --reporter=verbose"
  },
  "type": "module",
  "engines": {
    "node": ">=16.13.0"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@prisma/client": "^4.0.0",
    "@shopify/app-bridge": "^2.0.22",
    "@shopify/app-bridge-react": "^2.0.26",
    "@shopify/app-bridge-utils": "^2.0.26",
    "@shopify/polaris": "^9.14.1",
    "@shopify/polaris-icons": "^4.23.0",
    "@shopify/shopify-api": "^4.2.0",
    "@vitejs/plugin-react": "1.3.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "isomorphic-fetch": "^3.0.0",
    "mailgun-js": "^0.22.0",
    "node-schedule": "^2.1.0",
    "pg": "^8.7.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0",
    "redis": "^4.1.0",
    "serve-static": "^1.14.1",
    "styled-components": "^5.3.5",
    "vite": "^2.9.8",
    "winston": "^3.8.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mailgun-js": "^0.22.12",
    "@types/node-schedule": "^2.1.0",
    "@types/pg": "^8.6.5",
    "@types/styled-components": "^5.1.25",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^7.0.4",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "prisma": "^4.0.0",
    "supertest": "^6.2.3",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4",
    "vitest": "^0.10.5"
  },
  "keywords": [
    "shopify",
    "shopify-app",
    "shopify-embedded-app",
    "shopify-app-template",
    "shopify-app-typescript"
  ],
  "license": "MIT"
}
